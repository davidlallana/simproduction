	<div id="boxscroll">		<table class="normal sortable">						<tbody>							{% if compras==null %}					<p>No has realizado ninguna compra todavía</p>				{% else %}					<tr>						<th>Fecha</th>						<th>Empresa vendedora</th>						<th class="sorttable_numeric">Cantidad</th>						<th class="sorttable_numeric">Importe</th>					</tr>					{% for compra in compras %}							<tr class="normal">							<td>{{ compra.fecha | date('d/m') }}</td>							<td>{{ compra.empresaVendedora }}</td>							<td class="ancho150">{{ compra.cantidad }}{% if compra.empresaVendedora.sector.id==1 %} kg de mineral{% elseif compra.empresaVendedora.sector.id==2%} herramientas{% else %} vehículos{% endif %}</td>							<td class="ancho100">{{ compra.precio | number_format(0, ',', '.') }} €</td>						</tr>					{% endfor %}									{% endif %}									</tbody>		</table>				</div>	<br/>	{% block javascripts %}<script src="{{ asset('js/sorttable.js') }}"></script>	<script>	  $(document).ready(function() {		  		$("#boxscroll").niceScroll({touchbehavior:true});	  	  });	</script>	{% endblock %}